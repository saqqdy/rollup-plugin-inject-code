{
	"name": "rollup-plugin-inject-code",
	"description": "A rollup plugin for inject codes",
	"version": "1.0.0-beta.1",
	"packageManager": "pnpm@8.3.1",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"directories": {
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"dist",
		"typings"
	],
	"scripts": {
		"build": "run-s clean build:{bundle,es5,types,docs} roll-types",
		"build:bundle": "rollup -c build/rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build:es5": "cross-env BABEL_ENV=es5 pnpm build:bundle",
		"build:types": "tsc -p src/tsconfig.declaration.json",
		"build:docs": "rm-all docs && typedoc && prettier --write \"**/*.md\"",
		"roll-types": "api-extractor run && rm-all temp && cp dist/index.d.ts dist/es5",
		"watch": "pnpm build:bundle -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"workflow:publish-test": "zx scripts/workflow.mjs",
		"clean": "rm-all dist es lib",
		"dist": "run-s eslint prettier build",
		"test-unit": "jest --filter ./scripts/filter-unit.js",
		"jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
		"coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\""
	},
	"dependencies": {
		"magic-string": "^0.30.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@eslint-sets/eslint-config-ts": "^5.1.0",
		"@microsoft/api-extractor": "^7.34.4",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-typescript": "^11.1.0",
		"@types/jest": "^29.5.1",
		"@types/node": "^18.16.1",
		"chalk": "^5.2.0",
		"core-js": "^3.30.1",
		"coveralls": "^3.1.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.39.0",
		"fast-glob": "^3.2.12",
		"jest": "^29.5.0",
		"load-yml": "^1.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"prettier-config-common": "^1.4.0",
		"reinstaller": "^2.3.0",
		"rm-all": "^1.0.0",
		"rollup": "^3.21.0",
		"rollup-plugin-filesize": "^10.0.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"ts-jest": "^29.1.0",
		"tsnd": "^1.1.0",
		"typedoc": "^0.24.6",
		"typedoc-plugin-markdown": "^3.15.2",
		"typescript": "^5.0.4",
		"zx": "^7.2.1"
	},
	"peerDependencies": {
		"rollup": ">= 2.0.0"
	},
	"keywords": [
		"rollup",
		"plugin",
		"inject",
		"inject-code",
		"inject-plugin",
		"inject-iife",
		"saqqdy"
	],
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"license": "MIT",
	"author": "saqqdy <saqqdy@qq.com> (https://github.com/saqqdy)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/rollup-plugin-inject-code.git"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/rollup-plugin-inject-code/issues"
	},
	"homepage": "https://github.com/saqqdy/rollup-plugin-inject-code#readme"
}
